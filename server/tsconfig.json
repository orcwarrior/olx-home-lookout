{
  "module": "commonjs",
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "target": "es6",
  "noImplicitAny": true,
  "moduleResolution": "node",
  "sourceMap": true,
  "outDir": "dist",
  "listFiles": false,
  "traceResolution": false,
  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "downlevelIteration": true,
    "strict": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "paths": {
      "@config/*": [
        "config/*"
      ],
      "@api/*": [
        "api/*"
      ],
      "@db/*": [
        "db/*"
      ],
      "@logic/*": [
        "logic/*"
      ],
      "@events/*": [
        "api/events/*"
      ],
      "@root/*": [
        "*"
      ]
    },
    "lib": [
      "ESNext"
    ],
    "types": [
      "node"
    ]
  },
  "include": [
    "./api",
    "./config",
    "./db"
  ]
}
